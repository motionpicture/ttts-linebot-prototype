{
    "opts": {
        "template": "node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "README.md",
            "app",
            "bin"
        ],
        "exclude": []
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "systemName": "佐々木興行チケットシステムAPIウェブアプリケーション",
        "footer": "",
        "navType": "vertical",
        "theme": "cosmo",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "highlightTutorialCode": false,
        "protocol": "fred://"
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}